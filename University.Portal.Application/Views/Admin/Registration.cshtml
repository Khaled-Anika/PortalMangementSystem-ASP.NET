@model IEnumerable<University.Portal.Entites.RegistrationEntites.CourseRegistration>

@{
    ViewBag.Title = " ";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Registration
            <small>Page</small>
        </h1>

    </section>

    <!-- Main content -->
    <section class="content container-fluid">
        <div class="row">

            <div class="col-md-4 col-md-offset-1">
                <div class="login-box-body">
                    <p class="login-box-msg">Registration</p>

                    @using (Html.BeginForm("Registration", "Admin", FormMethod.Post, new { onSubmit = "return FormSubmit(this)" }))
                    {
                        <div class="form-group has-feedback">
                            <input type="text" class="form-control" placeholder="Enter ID" name="StudentId">
                        </div>


                        <div class="row">

                            <!-- /.col -->
                            <div class="col-xs-4">
                                <button type="submit" class="btn btn-danger  btn-flat">Submit</button>
                            </div>
                            <!-- /.col -->
                        </div>
                    }



                </div>
            </div>
        </div>


    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
